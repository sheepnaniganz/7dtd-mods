<?xml version="1.0" encoding="UTF-8"?>
<configs>
  <!-- *** Remove Cement and Re-add to Cement Mixer -->
  <remove xpath="//recipe[@name='resourceCement']" />
  <append xpath="/recipes">
    <recipe name="resourceCement" count="1" craft_area="cementMixer" tags="cementMixerCrafting">
      <ingredient name="resourceCrushedSand" count="2" />
      <ingredient name="resourceClayLump" count="2" />
    </recipe>
  </append>
</configs>
